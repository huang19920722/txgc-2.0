.video-title div{ margin-top:15px; line-height:46px;}
.video-title div p{ padding-top:5px;}
.video-title div p a{ display:block; height:36px; text-indent:20px;}
.video-title div .p1 a{ background: url(../images/video/head-off.png) no-repeat  left 17px; margin-right:16px;}
.video-title div .p2 a{ background:url(../images/video/hand-g.png) no-repeat left 14px;}
.video-title1{width:100%;height:55px; line-height:52px; color:#34847f; background:url(../images/d_line.png) repeat-x bottom; font-size:22px; margin-bottom:15px;}
.video-title1 span.s1{ float:left; height:52px; line-height:52px; padding:0 20px 0 40px; border-bottom:3px solid #4ab3ac; background:url(../images/s_icon.png) no-repeat 10px 16px;}
.video-title1 span.s2{ float:right; height:52px; line-height:52px; font-size:16px; color:#2d394f;}
.video-title1 span.s2 a{ color:#2d394f;}
.video-title1 span.s2 a:hover{color:#34847f;}
