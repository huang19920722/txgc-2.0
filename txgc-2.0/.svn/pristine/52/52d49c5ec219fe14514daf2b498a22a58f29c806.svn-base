.video-r ul li{padding-top:10px;}
.video-r ul p{ line-height:30px; font-size:14px;}
.video-r ul p span{float:left; padding-top:3px;}
.video-r .video-rtype{ height:35px;}
/*video重新播放*/
.next-box {position:relative;background-color: #2E3C45;font-size: 14px;text-align: center;color: #cfcfcf;}
.next-inner {margin-top: -643px;height: 443px; padding-top:200px;}
.next-line{background:#fff;height:200px; padding:35px; margin:0 auto; width:380px;border-radius:6px;}
.next-line h1{font-size:16px;color:#3a3a3a;line-height:35px; text-align:left; margin-bottom:10px; font-weight:normal;}
.next-line font{font-size:16px; color:#3a3a3a;}
.next-inner .logo_text{font-size:24px; background:#fff;height:35px;margin-bottom:12px;color:#3a3a3a; border:1px solid #c9c9c9; border-radius:6px;}
.next-inner input{font-size:15px; border:0; height:33px;vertical-align:middle;}
.next-inner img{vertical-align:middle;}
.next-inner .btn-exam{vertical-align:middle; text-align:center;}
.next-inner .btn-refer{margin-left:68px;}
.next-box a.again {background: url(../images/video/refresh.png) no-repeat left 5px;width:100px;height: 63px;display: block;margin: 8px auto;padding-top: 8px;padding-left: 35px;color: #fff;font-size: 20px;}
.next-box a.again:hover{background: url(../images/video/refresh-hover.png) no-repeat left 5px;width: 100px;height: 63px;}
.next-box a.btn-exam {font-size: 24px;}
.btn-exam.btn-xlarge {height: 60px;line-height: 60px;overflow: visible;font-size: 36px;padding: 0 30px;}
.btn-exam,a.btn-exam {
cursor: pointer;
border: none;
background: #c9394a;
color: #ffffff;
padding: 0 15px;
font-size: 14px;
display: inline-block;
-webkit-transition: 0.25s;
-moz-transition: 0.25s;
-o-transition: 0.25s;
transition: 0.25s;
font-family: '微软雅黑';
margin: 10px 0;
height: 26px;
overflow: visible;
line-height: 26px;
text-decoration: none;
}
