.video-r{ width:280px;}
.video-r .i-bnt{ height:21px; line-height:21px; background:#f7a947; font-size:13px;}
.video-r dl{line-height:30px; font-size:13px; color:#3e3e3e; padding:0px 12px 10px; background:#fff;border-bottom:1px solid #ddd; margin-top:20px; overflow:hidden;}
.video-r dt{ font-size:16px; color:#000; line-height:35px; margin-top:5px;}
.video-r dt.st1{ font-size:18px; color:#2D3750; font-weight:bold;}
